<config xmlns="urn:ietf:params:xml:ns:netconf:base:1.0">
    <configuration xmlns="http://xml.juniper.net/xnm/1.1/xnm" xmlns:junos="http://xml.juniper.net/junos/commit" junos:commit-user="root">
        <system>
            <host-name>F1</host-name>
        </system>
        <security>
                        <address-book>
                <name>test1234</name>
                                <address>
                    <name>ue_subnet</name>
                    <range-address>
                        <name>192.168.1.0</name>
                        <to>
                            <range-high>192.168.1.254</range-high>
                        </to>
                    </range-address>
                </address>
                                <attach>
                    <zone>
                        <name>trust</name>
                    </zone>
                </attach>
            </address-book>
                        <address-book>
                <name>test5678</name>
                                <address>
                    <name>outside_network</name>
                    <range-address>
                        <name>1.1.1.0</name>
                        <to>
                            <range-high>1.1.1.254</range-high>
                        </to>
                    </range-address>
                </address>
                                <attach>
                    <zone>
                        <name>untrust</name>
                    </zone>
                </attach>
            </address-book>
                    </security>
    </configuration>
</config>
