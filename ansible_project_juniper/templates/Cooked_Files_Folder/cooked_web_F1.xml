<config xmlns="urn:ietf:params:xml:ns:netconf:base:1.0">
    <configuration xmlns="http://xml.juniper.net/xnm/1.1/xnm" xmlns:junos="http://xml.juniper.net/junos/commit" junos:commit-user="root">
        <system>
            <host-name>F1</host-name>
        </system>
        <security>
		    <utm>
			    <feature-profile>
				   <web-filtering>
				       <juniper-enhanced>
					       					       <profile>
						        <name>test1</name>
							    <category>
							       <name>Enhanced_Abused_Drugs</name>
								   <action>log-and-permit</action>
								</category>
								<site-reputation-action>
									<very-safe>log-and-permit</very-safe>
									<moderately-safe>log-and-permit</moderately-safe>
									<fairly-safe>log-and-permit</fairly-safe>
									<suspicious>quarantine</suspicious>
									<harmful>block</harmful>
								</site-reputation-action>
								<default>log-and-permit</default>
								<fallback-settings>
								     <default>log-and-permit</default>
								</fallback-settings>
							</profile>
												       <profile>
						        <name>test2</name>
							    <category>
							       <name>Enhanced_Gambling</name>
								   <action>log-and-permit</action>
								</category>
								<site-reputation-action>
									<very-safe>log-and-permit</very-safe>
									<moderately-safe>log-and-permit</moderately-safe>
									<fairly-safe>log-and-permit</fairly-safe>
									<suspicious>quarantine</suspicious>
									<harmful>block</harmful>
								</site-reputation-action>
								<default>log-and-permit</default>
								<fallback-settings>
								     <default>log-and-permit</default>
								</fallback-settings>
							</profile>
													</juniper-enhanced>
					</web-filtering>
				</feature-profile>
				<utm-policy>
				    <name>web99060</name>
					<web-filtering>
					    <http-profile>test1</http-profile>
					</web-filtering>
				</utm-policy>
			</utm>
        </security>
    </configuration>
</config>
